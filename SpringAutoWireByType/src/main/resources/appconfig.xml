<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
                            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<bean id="employee" class="com.bridgeit.SpringAutoWireByType.Employee" autowire="byType"></bean>
	
	<bean id="employeeaddress" class="com.bridgeit.SpringAutoWireByType.EmployeeAddress">
		<property name="streat" value="Shivaji Road"></property>
		<property name="city" value="Thane"></property>
		<property name="state" value="Maharashtra"></property>
	</bean>

</beans>