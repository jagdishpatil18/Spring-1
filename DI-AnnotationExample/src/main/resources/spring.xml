<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
                            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">
 
    <bean id="activeMqMessaging" class="com.bridgeit.DI_AnnotationExample.implementing_class.ActiveMQMessaging" />
 
    <bean id="rsaEncryption" class="com.bridgeit.DI_AnnotationExample.implementing_class.RSAEncryption" />
 
    <bean id="communication" class="com.bridgeit.DI_AnnotationExample.bean.Communication">
        <constructor-arg>
            <ref bean="rsaEncryption" />
        </constructor-arg>
        <property name="messaging">
            <ref bean="activeMqMessaging" />
        </property>
    </bean>
 
</beans>